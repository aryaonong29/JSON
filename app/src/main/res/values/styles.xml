<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="tv1">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text"></item>
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_alignParentStart">true</item>
        <item name="android:textSize">24dp</item>
    </style>

    <style name="tv2">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">Address</item>
        <item name="android:layout_below">@+id/tv1</item>
        <item name="android:layout_alignParentStart">true</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:textSize">24dp</item>
        <item name="android:layout_marginTop">40dp</item>
    </style>

    <style name="tv3">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text"></item>
        <item name="android:layout_below">@+id/tv2</item>
        <item name="android:layout_alignParentStart">true</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:textSize">24dp</item>
        <item name="android:layout_marginTop">29dp</item>
    </style>

    <style name="tv4">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text"></item>
        <item name="android:layout_below">@+id/tv3</item>
        <item name="android:layout_alignParentStart">true</item>
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:textSize">24dp</item>
        <item name="android:layout_marginTop">26dp</item>
    </style>

</resources>
